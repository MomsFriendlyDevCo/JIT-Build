{
  "name": "@momsfriendlydevco/jit-build",
  "version": "1.0.5",
  "description": "Just-in-time, as-required dynamic build endpoints via Express",
  "main": "lib/es6.js",
  "type": "module",
  "scripts": {
    "lint": "eslint lib test",
    "postinstall": "patch-package",
    "test": "mocha"
  },
  "keywords": [
    "jit",
    "express",
    "esbuild"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MomsFriendlyDevCo/JIT-Build.git"
  },
  "bugs": {
    "url": "https://github.com/MomsFriendlyDevCo/JIT-Build/issues"
  },
  "homepage": "https://github.com/MomsFriendlyDevCo/JIT-Build#readme",
  "author": "Matt Carter <m@ttcarter.com>",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "exports": {
    ".": "./lib/jit.js",
    "./*": "./lib/*.js"
  },
  "imports": {
    "#lib/*": "./lib/*.js",
    "#plugins/*": "./plugins/*.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "mocha": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 13,
      "sourceType": "module"
    }
  },
  "dependencies": {
    "@momsfriendlydevco/formatters": "^1.2.3",
    "chalk": "^5.2.0",
    "debug": "^4.3.4",
    "esbuild": "^0.17.7",
    "esbuild-sass-plugin": "^2.4.5",
    "esbuild-vue": "^1.2.2",
    "globby": "^13.1.3",
    "lodash": "^4.17.21",
    "patch-package": "^6.5.1"
  },
  "devDependencies": {
    "@momsfriendlydevco/es6": "^1.2.0",
    "axios": "^1.3.2",
    "chai": "^4.3.7",
    "eslint": "^8.33.0",
    "express": "^4.18.2",
    "express-log-url": "^1.5.1",
    "mocha": "^10.2.0",
    "mocha-logger": "^1.0.8"
  }
}
